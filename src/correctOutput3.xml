<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE log4j:configuration SYSTEM "log4j.dtd">
<log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/" debug="true" reset="false" threshold="all">
    <appender class="org.apache.log4j.RollingFileAppender" name="FILE_LOG">
        <param name="Append" value="true"/>
        <param name="File" value="${app.home}/WEB-INF/logs/study.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="[%d{ABSOLUTE}] %p: %l: %m %n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.ConsoleAppender" name="C">
        <layout class="org.apache.log4j.SimpleLayout"/>
    </appender>
    <appender class="org.apache.log4j.ConsoleAppender" name="stdout">
        <param name="Target" value="System.out"/>
        <param name="target" value="System.err"/>
        <layout class="org.apache.log4j.TTCCLayout">
            <param name="conversionPattern"
                   value="%d [%X{loginUserId}/%X{req.remoteAddr}/%X{req.id} - %X{entranceMethod} - %X{req.requestURIWithQueryString}] %-5p %c - %m%n"/>
            <param name="ConversionPattern" value="[%5p] %d{mm:ss} (%F:%M:%L)%n%m%n%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.DailyRollingFileAppender" name="common-error">
        <param name="file" value="${user.home}/logs/common-error.log"/>
        <param name="threshold" value="ERROR"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern"
                   value="%d [%X{loginUserId}/%X{req.remoteAddr}/%X{req.id} - %X{entranceMethod} - %X{req.requestURIWithQueryString}] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.wso2.carbon.logging.appenders.MemoryAppender" name="CARBON_MEMORY">
        <param name="threshold" value="DEBUG"/>
        <param name="bufferSize" value="200"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="[%d] %5p - %x %m {%c}%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="H">
        <param name="Append" value="false"/>
        <param name="MaxFileSize" value="100KB"/>
        <param name="File" value="logs/gitblit.html"/>
        <layout class="org.apache.log4j.HTMLLayout"/>
    </appender>
    <appender class="org.apache.log4j.DailyRollingFileAppender" name="project-web">
        <param name="file" value="${user.home}/logs/project-web.log"/>
        <param name="threshold" value="INFO"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern"
                   value="%d [%X{loginUserId}/%X{req.remoteAddr}/%X{req.id} - %X{entranceMethod} - %X{req.requestURIWithQueryString}] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.FileAppender" name="CATALINA">
        <param name="file" value="${catalina.base}/logs/catalina.log"/>
        <param name="encoding" value="UTF-8"/>
        <param name="append" value="true"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern" value="%d [%t] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="DebugAppender">
        <param name="Threshold?BUG" value=""/>
        <param name="MaxBackupIndex" value=""/>
        <param name="MaxFileSizeP0KB" value=""/>
        <param name="File" value="C:\logs\app-debug.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%c %d{ISO8601} -- %p -- %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="TomcatLogFile">
        <param name="Append" value="true"/>
        <param name="MaxBackupIndex" value="10"/>
        <param name="MaxFileSize" value="10000KB"/>
        <param name="File" value="/path/to/log/file/biomodels-tomcat.log"/>
        <param name="Threshold" value="DEBUG"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%d %-5p %-17c{2} (%13F:%L) %3x - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="A1">
        <param name="MaxBackupIndex" value="500"/>
        <param name="MaxFileSize" value="1048576"/>
        <param name="File" value="${log.dir}/dspace.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%-6r %-5p [%t] %-17c{2} (%13F:%L) %3x - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="rolling">
        <param name="MaxBackupIndex" value="100"/>
        <param name="MaxFileSize" value="10MB"/>
        <param name="File" value="sonar-replay.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%d{ISO8601} %-6r [%15.15t] %-5p %30.30c %x - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="A2">
        <param name="MaxBackupIndex" value="500"/>
        <param name="Append" value="false"/>
        <param name="MaxFileSize" value="1048576"/>
        <param name="File" value="org.apache.ojb.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%-5r %-5p [%t] %c{2} - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="InfoAppender">
        <param name="MaxBackupIndex" value=""/>
        <param name="MaxFileSizeP0KB" value=""/>
        <param name="File" value="C:\logs\app-info.log"/>
        <param name="Threshold" value="INFO"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%c %d{ISO8601} -- %p -- %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="ErrorAppender">
        <param name="MaxBackupIndex" value=""/>
        <param name="MaxFileSizeP0KB" value=""/>
        <param name="File" value="C:\logs\app-error.log"/>
        <param name="Threshold" value="ERROR"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%c %d{ISO8601} -- %p -- %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.DailyRollingFileAppender" name="R">
        <param name="Append" value="true"/>
        <param name="File" value="logs/gitblit.log"/>
        <param name="DatePattern" value="'.'yyyy-MM-dd"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss} %c{1} [%p] %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="file">
        <param name="MaxBackupIndex" value="1"/>
        <param name="MaxFileSize" value="1MB"/>
        <param name="File" value="C:\loging.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%d{ABSOLUTE} %5p %c{1}:%L - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.ConsoleAppender" name="S">
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%-5p %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.FileAppender" name="xml">
        <param name="file" value="sonar-replay-xml.log"/>
        <param name="append" value="false"/>
        <layout class="org.apache.log4j.xml.XMLLayout"/>
    </appender>
    <appender class="org.apache.log4j.FileAppender" name="MANAGER">
        <param name="file" value="${catalina.base}/logs/manager.log"/>
        <param name="encoding" value="UTF-8"/>
        <param name="append" value="true"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern" value="%d [%t] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.DailyRollingFileAppender" name="project-service">
        <param name="file" value="${user.home}/logs/project-service.log"/>
        <param name="threshold" value="INFO"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern"
                   value="%d [%X{loginUserId}/%X{req.remoteAddr}/%X{req.id} - %X{entranceMethod} - %X{req.requestURIWithQueryString}] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.FileAppender" name="HOST-MANAGER">
        <param name="file" value="${catalina.base}/logs/host-manager.log"/>
        <param name="encoding" value="UTF-8"/>
        <param name="append" value="true"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern" value="%d [%t] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.DailyRollingFileAppender" name="common-default">
        <param name="file" value="${user.home}/logs/common-default.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern"
                   value="%d [%X{loginUserId}/%X{req.remoteAddr}/%X{req.id} - %X{entranceMethod} - %X{req.requestURIWithQueryString}] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="lf5Rolling">
        <param name="File" value="example_lf5.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern"
                   value="[slf5s.start]%d{DATE}[slf5s.DATE]%n%p[slf5s.PRIORITY]%n%x[slf5s.NDC]%n%t[slf5s.THREAD]%n%c[slf5s.CATEGORY]%n%l[slf5s.LOCATION]%n%m[slf5s.MESSAGE]%n%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.ConsoleAppender" name="console">
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="accessLogFile">
        <param name="MaxBackupIndex" value="1"/>
        <param name="MaxFileSize" value="4MB"/>
        <param name="File" value="C:/Corda/CenterView3/Server/logs/access.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%d [%t] %p %c = %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="logfile">
        <param name="MaxFileSize" value="2048KB"/>
        <param name="File" value="logs/log.txt"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%d %p - &lt;%m&gt;%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="MAIN_LOG">
        <param name="MaxBackupIndex" value="99"/>
        <param name="MaxFileSize" value="10MB"/>
        <param name="File" value="${catalina.base}/logs/webtop.log"/>
        <param name="append" value="true"/>
        <param name="DatePattern" value="'.'yyyy-MM-dd"/>
        <layout class="com.mycompany.util.log.Log4JSimpleLayout"/>
    </appender>
    <appender class="org.apache.log4j.ConsoleAppender" name="CARBON_CONSOLE">
        <param name="threshold" value="DEBUG"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="[%d] %5p {%c} - %x %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="WarnAppender">
        <param name="MaxBackupIndex" value=""/>
        <param name="MaxFileSizeP0KB" value=""/>
        <param name="File" value="C:\logs\app-warn.log"/>
        <param name="Threshold" value="WARN"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%c %d{ISO8601} -- %p -- %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.DailyRollingFileAppender" name="common-pageNotFound">
        <param name="file" value="${user.home}/logs/common-pageNotFound.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern"
                   value="%d [%X{loginUserId}/%X{req.remoteAddr}/%X{req.id} - %X{entranceMethod} - %X{req.requestURIWithQueryString}] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.DailyRollingFileAppender" name="common-warn">
        <param name="file" value="${user.home}/logs/common-warn.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern"
                   value="%d [%X{loginUserId}/%X{req.remoteAddr}/%X{req.id} - %X{entranceMethod} - %X{req.requestURIWithQueryString}] %-5p %c - %m%n"/>
        </layout>
        <filter class="org.apache.log4j.varia.LevelRangeFilter">
            <param name="levelMax" value="WARN"/>
            <param name="levelMin" value="WARN"/>
        </filter>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="SBMLDebugLogFile">
        <param name="MaxBackupIndex" value="10"/>
        <param name="Append" value="true"/>
        <param name="MaxFileSize" value="10000KB"/>
        <param name="File" value="/path/to/log/file/biomodels-sbmllib.log"/>
        <param name="Threshold" value="DEBUG"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%d %-5p %-17c{2} (%13F:%L) %3x - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.ConsoleAppender" name="CONSOLE">
        <param name="encoding" value="UTF-8"/>
        <param name="Threshold" value="DEBUG"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern" value="%d [%t] %-5p %c - %m%n"/>
            <param name="ConversionPattern" value="%d %p: %m%n"/>
        </layout>
        <filter class="org.apache.log4j.varia.StringMatchFilter">
            <param name="StringToMatch" value="${my.StringToMatch}"/>
        </filter>
        <filter class="org.apache.log4j.varia.LevelMatchFilter">
            <param name="LevelToMatch" value="WARN"/>
            <param name="AcceptOnMatch" value="false"/>
        </filter>
        <filter class="org.apache.log4j.varia.LevelMatchFilter">
            <param name="LevelToMatch" value="${my.LevelToMatch}"/>
        </filter>
        <filter class="org.apache.log4j.varia.LevelMatchFilter">
            <param name="NoSuchProperty" value="no-such-value"/>
        </filter>
        <filter class="org.apache.log4j.varia.DenyAllFilter"/>
        <filter class="filterClass">
            <param name="param1" value="filterParm1"/>
            <param name="param2" value="filterParm2"/>
        </filter>
        <filter class="filterClass">
            <param name="param3" value="filterParm3"/>
            <param name="param1" value="filterParm1"/>
            <param name="param2" value="filterParm2"/>
        </filter>
        <filter class="filterClass">
            <param name="param1" value="filterParm1"/>
        </filter>
        <filter class="filterClass">
            <param name="param1" value="filterParm1"/>
        </filter>
        <filter class="filterClass">
            <param name="param1" value="filterParm1"/>
        </filter>
        <filter class="filterClass">
            <param name="param3" value="filterParm3"/>
            <param name="param4" value="filterParm4"/>
            <param name="param1" value="filterParm1"/>
            <param name="param2" value="filterParm2"/>
        </filter>
    </appender>
    <appender class="org.apache.log4j.RollingFileAppender" name="BioModelsDebugLogFile">
        <param name="Append" value="true"/>
        <param name="MaxBackupIndex" value="10"/>
        <param name="MaxFileSize" value="10000KB"/>
        <param name="File" value="/path/to/log/file/biomodels-debug.log"/>
        <param name="Threshold" value="DEBUG"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="ConversionPattern" value="%d %-5p %-17c{2} (%13F:%L) %3x - %m%n"/>
        </layout>
    </appender>
