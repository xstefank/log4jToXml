        </layout>
    </appender>
    <appender class="org.apache.log4j.FileAppender" name="LOCALHOST">
        <param name="file" value="${catalina.base}/logs/localhost.log"/>
        <param name="encoding" value="UTF-8"/>
        <param name="append" value="true"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern" value="%d [%t] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.DailyRollingFileAppender" name="common-web">
        <param name="file" value="${user.home}/logs/common-web.log"/>
        <layout class="org.apache.log4j.PatternLayout">
            <param name="conversionPattern"
                   value="%d [%X{loginUserId}/%X{req.remoteAddr}/%X{req.id} - %X{entranceMethod} - %X{req.requestURIWithQueryString}] %-5p %c - %m%n"/>
        </layout>
    </appender>
    <appender class="org.apache.log4j.lf5.LF5Appender" name="lf5">
        <param name="MaxNumberOfRecords" value="1000"/>
    </appender>
    <appender class="org.apache.log4j.net.SocketAppender" name="socketLogger">
        <param name="RemoteHost" value="localhost"/>
        <param name="Port" value="4445"/>
        <param name="LocationInfo" value="false"/>
    </appender>
